{
    "name": "chameleon/laravel-gelf",
    "description": "Laravel Gelf",
    "minimum-stability": "stable",
    "keywords": [
        "laravel",
        "laravel-gelf",
        "log",
        "graylog",
        "graylog2",
        "gelf driver",
        "graylog driver"
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "^5.7|^6.0|^7.0|^8.0",
        "graylog2/gelf-php": "1.*",
        "monolog/monolog": "^1.12|^2.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelGelf\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelGelf\\LaravelGelfProvider"
            ]
        }
    }
}
